<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.seasar.doma.extension.domax.category"
            id="org.seasar.doma.extension.domax.commands.jumpToDao"
            name="Jump to Dao">
      </command>
      <command
            categoryId="org.seasar.doma.extension.domax.category"
            id="org.seasar.doma.extension.domax.commands.jumpToSqlFile"
            name="Jump to Sql File">
      </command>
      <category
            id="org.seasar.doma.extension.domax.category"
            name="Doma">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.seasar.doma.extension.domax.handler.ResourcePopupToJavaEditorHandler"
            commandId="org.seasar.doma.extension.domax.commands.jumpToDao">
         <activeWhen>
            <with
                  variable="selection">
               <count
                     value="1">
               </count>
               <iterate>
                  <adapt
                        type="org.eclipse.core.resources.IFile">
                     <or>
                        <test
                              property="org.eclipse.core.resources.extension"
                              value="sql">
                        </test>
                        <test
                              property="org.eclipse.core.resources.extension"
                              value="script">
                        </test>
                     </or>
                  </adapt>
               </iterate>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="org.seasar.doma.extension.domax.handler.ResourceEditorToJavaEditorHandler"
            commandId="org.seasar.doma.extension.domax.commands.jumpToDao">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        operator="or">
                     <instanceof
                           value="org.eclipse.jface.text.TextSelection">
                     </instanceof>
                  </iterate>
               </with>
               <and>
                  <with
                        variable="activeEditorId">
                     <not>
                        <equals
                              value="org.eclipse.jdt.ui.CompilationUnitEditor">
                        </equals>
                     </not>
                  </with>
                  <with
                        variable="activeEditorId">
                     <not>
                        <equals
                              value="com.google.gwt.eclipse.core.editors.gwtJavaEditor">
                        </equals>
                     </not>
                  </with></and>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="org.seasar.doma.extension.domax.handler.JavaEditorToResourceEditorHandler"
            commandId="org.seasar.doma.extension.domax.commands.jumpToSqlFile">
         <activeWhen>
            <and>
               <with
                     variable="selection">
                  <iterate
                        operator="or">
                     <instanceof
                           value="org.eclipse.jface.text.TextSelection">
                     </instanceof>
                  </iterate>
               </with>
               <or>
                  <with
                        variable="activeEditorId">
                     <equals
                           value="org.eclipse.jdt.ui.CompilationUnitEditor">
                     </equals>
                  </with>
                  <with
                        variable="activeEditorId">
                     <equals
                           value="com.google.gwt.eclipse.core.editors.gwtJavaEditor">
                     </equals>
                  </with>
               </or>
            </and>
         </activeWhen>
      </handler>
      <handler
            class="org.seasar.doma.extension.domax.handler.JavaPopupToResourceEditorHandler"
            commandId="org.seasar.doma.extension.domax.commands.jumpToSqlFile">
         <activeWhen>
            <with
                  variable="selection">
               <count
                     value="1">
               </count>
               <iterate>
                  <adapt
                        type="org.eclipse.jdt.core.IMethod">
                  </adapt>
               </iterate>
            </with>
         </activeWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.seasar.doma.extension.domax.commands.jumpToDao"
            contextId="org.eclipse.ui.textEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+0">
      </key>
      <key
            commandId="org.seasar.doma.extension.domax.commands.jumpToSqlFile"
            contextId="org.eclipse.jdt.ui.javaEditorScope"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M3+0">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="menu"
               label="Doma">
            <command
                  commandId="org.seasar.doma.extension.domax.commands.jumpToDao"
                  label="Jump to Dao"
                  mnemonic="J"
                  style="push">
               <visibleWhen>
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate>
                        <adapt
                              type="org.eclipse.core.resources.IFile">
                           <or>
                              <test
                                    property="org.eclipse.core.resources.extension"
                                    value="sql">
                              </test>
                              <test
                                    property="org.eclipse.core.resources.extension"
                                    value="script">
                              </test>
                           </or>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="menu"
               label="Doma">
            <command
                  commandId="org.seasar.doma.extension.domax.commands.jumpToSqlFile"
                  label="Jump to SQL File"
                  mnemonic="J"
                  style="push">
               <visibleWhen>
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate>
                        <adapt
                              type="org.eclipse.jdt.core.IMethod">
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            locationURI="popup:#TextEditorContext?after=additions">
  
         <menu
               id="menu"
               label="Doma">
            <command
                  commandId="org.seasar.doma.extension.domax.commands.jumpToDao"
                  label="Jump to Dao"
                  mnemonic="J"
                  style="push">
               <visibleWhen>
                  <and>
                     <with
                           variable="selection">
                        <iterate
                              operator="or">
                           <instanceof
                                 value="org.eclipse.jface.text.TextSelection">
                           </instanceof>
                        </iterate>
                     </with>
                     <and>
                        <with
                              variable="activeEditorId">
                           <not>
                              <equals
                                    value="org.eclipse.jdt.ui.CompilationUnitEditor">
                              </equals>
                           </not>
                        </with>
                        <with
                              variable="activeEditorId">
                           <not>
                              <equals
                                    value="com.google.gwt.eclipse.core.editors.gwtJavaEditor">
                              </equals>
                           </not>
                        </with>
                     </and>
                  </and>
               </visibleWhen>
            </command>
         </menu>    </menuContribution>
      <menuContribution
            locationURI="popup:#CompilationUnitEditorContext?after=additions">
         <menu
               id="menu"
               label="Doma">
            <command
                  commandId="org.seasar.doma.extension.domax.commands.jumpToSqlFile"
                  label="Jump to SQL File"
                  mnemonic="J"
                  style="push">
               <visibleWhen>
                  <and>
                     <with
                           variable="selection">
                        <iterate
                              operator="or">
                           <instanceof
                                 value="org.eclipse.jface.text.TextSelection">
                           </instanceof>
                        </iterate>
                     </with>
                     <or>
                        <with
                              variable="activeEditorId">
                           <equals
                                 value="org.eclipse.jdt.ui.CompilationUnitEditor">
                           </equals>
                        </with>
                        <with
                              variable="activeEditorId">
                           <equals
                                 value="com.google.gwt.eclipse.core.editors.gwtJavaEditor">
                           </equals>
                        </with>
                     </or>
                  </and>
               </visibleWhen>
            </command>
         </menu>
      </menuContribution>
   </extension>
</plugin>
